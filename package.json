    "@types/express": "^4.17.17",
    "@types/jasmine": "~4.3.0",
    "@types/jest": "^29.5.11",
    "@types/node": "^18.18.0",
    "jest": "^29.7.0",
    "jest-preset-angular": "^13.1.4",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  }
